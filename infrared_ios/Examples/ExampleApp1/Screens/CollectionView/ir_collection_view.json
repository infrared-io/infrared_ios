{
  "id" : "ir_collection_view",
  "controller" : {
    "id" : "ir_collection_view_vc",
    "controllerType" : "viewController",
    "supportedInterfaceOrientations" : "UIInterfaceOrientationPortrait | UIInterfaceOrientationPortraitUpsideDown | UIInterfaceOrientationLandscapeLeft | UIInterfaceOrientationLandscapeRight",
    "title" : "Picker view",
    "jsPluginPath" : "ir_collection_view_plugin.js",
    "navigationController" : {
      "autoAddIfNeeded" : true,
      "hideNavigationBar" : false
    }
  },
  "view" : {
    "id" : "root_view",
    "backgroundColor" : "#00ff00",
    "subviews" : [
      {
        "id" : "collection_view",
        "type" : "collectionView",
        "backgroundColor" : "#ffffff",
        "selectItemAction": "this.selectItem1();",
        "sectionEdgeInsets": {
          "top": 50,
          "left": 20,
          "bottom": 50,
          "right": 20
        },
        "dataBinding" : [
          {
            "property" : "collectionData",
            "data" : "this.collectionData"
          }
        ],
        "cells": [
          {
            "id": "default_cell",
            "type": "collectionViewCell",
            "backgroundColor": "#00ff0080",
            "selectItemAction_": "this.selectItem2();",
            "cellSize": {
              "width": 100,
              "height": 100
            },
            "subviews": [
              {
                "id": "title_label",
                "type": "label",
                "text": "Title",
                "backgroundColor": "#ff000080",
                "textColor": "#ffffff",
                "font": "SystemBold, 18"
              }
            ],
            "constraints": [
              {
                "visualFormat": "H:|-[title_label]-|"
              },
              {
                "visualFormat": "V:|-[title_label]"
              }
            ]
          }
        ]
      }
    ],
    "constraints" : [
      {
        "visualFormat" : "H:|-0-[collection_view]-0-|"
      },
      {
        "visualFormat" : "V:|-0-[collection_view]-0-|"
      }
    ]
  }
}