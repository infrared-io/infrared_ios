{
  "id" : "ir_collection_view",
  "controller" : {
    "id" : "ir_collection_view_vc",
    "controllerType" : "viewController",
    "supportedInterfaceOrientations" : "UIInterfaceOrientationPortrait | UIInterfaceOrientationPortraitUpsideDown | UIInterfaceOrientationLandscapeLeft | UIInterfaceOrientationLandscapeRight",
    "title" : "Picker view",
    "jsPluginPath" : "ir_collection_view_plugin.js",
    "navigationController" : {
      "autoAddIfNeeded" : true,
      "hideNavigationBar" : false
    }
  },
  "view" : {
    "id" : "root_view",
    "backgroundColor" : "#00ff00",
    "subviews" : [
      {
        "id" : "collection_view",
        "type" : "collectionView",
        "backgroundColor" : "#ffffff",
        "selectItemAction": "this.selectItem1();",
        "scrollDirection": "UICollectionViewScrollDirectionVertical",
        "userInteractionEnabled": true,
        "sectionEdgeInsets": {
          "top": 6,
          "left": 20,
          "bottom": 6,
          "right": 20
        },
        "sectionHeaderHeight": 10,
        "sectionFooterHeight": 10,
        "cellSize": {
          "width": 100,
          "height": 100
        },
        "dataBinding" : [
          {
            "property" : "collectionData",
            "data" : "this.collectionData"
          }
        ],
        "sectionHeaders": [
          {
            "id" : "section_header_1",
            "type" : "collectionReusableView",
            "backgroundColor" : "#f3f3f3",
            "subviews": [
              {
                "id": "header_label",
                "type": "label",
                "text": "Header",
                "textAlignment": "NSTextAlignmentCenter",
                "backgroundColor": "#ff000080",
                "textColor": "#ffffff",
                "font": "SystemBold, 16"
              }
            ],
            "constraints": [
              {
                "visualFormat": "H:|-[header_label]-|"
              },
              {
                "visualFormat": "V:|-0-[header_label]-0-|"
              }
            ]
          }
        ],
        "sectionFooters": [
          {
            "id" : "section_footer_1",
            "type" : "collectionReusableView",
            "backgroundColor" : "#f3f3f3",
            "subviews": [
              {
                "id": "footer_label",
                "type": "label",
                "text": "Footer",
                "textAlignment": "NSTextAlignmentCenter",
                "backgroundColor": "#0000ff80",
                "textColor": "#ffffff",
                "font": "SystemBold, 16"
              }
            ],
            "constraints": [
              {
                "visualFormat": "H:|-[footer_label]-|"
              },
              {
                "visualFormat": "V:|-0-[footer_label]-0-|"
              }
            ]
          }
        ],
        "cells": [
          {
            "id": "default_cell",
            "type": "collectionViewCell",
            "userInteractionEnabled": true,
            "backgroundColor": "#00ff0080",
            "selectItemAction_": "this.selectItem2();",
            "cellSize": {
              "width": 100,
              "height": 100
            },
            "subviews": [
              {
                "id": "title_label",
                "type": "label",
                "text": "Title",
                "backgroundColor": "#ff000080",
                "textColor": "#ffffff",
                "font": "SystemBold, 18",
                "userInteractionEnabled": false
              }
            ],
            "constraints": [
              {
                "visualFormat": "H:|-[title_label]-|"
              },
              {
                "visualFormat": "V:|-[title_label]"
              }
            ]
          }
        ]
      }
    ],
    "constraints" : [
      {
        "visualFormat" : "H:|-0-[collection_view]-0-|"
      },
      {
        "visualFormat" : "V:|-0-[collection_view]-0-|"
      }
    ]
  }
}